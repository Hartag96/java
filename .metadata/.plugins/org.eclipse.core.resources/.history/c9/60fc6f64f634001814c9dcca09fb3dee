

public class PO6 {
public class NrTelefoniczny implements Comparable {
int nrkierunkowy;
int nrTelefonu;
public void check() {
    System.out.println("Number checked ");

}
}
public class Osoba extends Wpis {
String imie;
String nazwisko;
String adres;

// nrTelefonu
public Osoba(String imie, String nazwisko, String adres){
        this.imie = imie;
        this.nazwisko = nazwisko;
        this.adres = adres;
}
public void opis() {
  System.out.println("Osoba: " + imie + " " + nazwisko + "  Adres: " + adres);
}
}

public class Firma extends Wpis {
String nazwa;
String adres ;
// nrTelefonu
public Firma(String nazwa, String adres){
        this.nazwa = nazwa;
        this.adres = adres;
}
public void opis() {
  System.out.println("Firma: " + nazwa + " Adres: " + adres);
}
}

public abstract class Wpis {
public abstract void opis();
}

public interface Comparable {
public void check();
}

public static void main(String[] args) {
		Osoba ja = new Osoba("Przemek", "Prencel", "babice");
        System.out.println("Rownanie nie ma pierwiastkow");

}

}