import PO6.Wpis;

public class Osoba extends Wpis {
String imie;
String nazwisko;
String adres;

// nrTelefonu
public Osoba(String imie, String nazwisko, String adres){
        this.imie = imie;
        this.nazwisko = nazwisko;
        this.adres = adres;
}
public void opis() {
  System.out.println("Osoba: " + imie + " " + nazwisko + "  Adres: " + adres);
}
}